{
  "npmScope": "reslife",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "boarder-management-feature": {
      "tags": ["type:feature"]
    },
    "check-ins-check-in-data-access": {
      "tags": ["type:data-access"]
    },
    "check-ins-check-in-feature": {
      "tags": ["type:feature"]
    },
    "check-ins-check-in-model": {
      "tags": ["type:model"]
    },
    "check-ins-check-in-ui": {
      "tags": ["type:ui"]
    },
    "reslife-app": {
      "tags": ["type:app"]
    },
    "reslife-app-e2e": {
      "tags": [],
      "implicitDependencies": ["reslife-app"]
    },
    "reslife-request": {
      "tags": ["type:app"]
    },
    "reslife-request-e2e": {
      "tags": [],
      "implicitDependencies": ["reslife-request"]
    },
    "shared-models": {
      "tags": ["type:model"]
    },
    "shared-ui": {
      "tags": ["type:ui"]
    },
    "utils": {
      "tags": ["type:util"]
    },
    "welcome-feature": {
      "tags": ["type:feature"]
    }
  }
}
